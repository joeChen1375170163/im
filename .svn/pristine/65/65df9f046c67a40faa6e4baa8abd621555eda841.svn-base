/*滚动条样式*/
/*---滚动条默认显示样式--*/
::-webkit-scrollbar-thumb {
	background-color: #b6becc;
	height: 50px;
	outline-offset: -2px;
	outline: 1px solid #fff;
	-webkit-border-radius: 6px;
	border: 1px solid #fff;
}
/*---鼠标点击滚动条显示样式--*/
::-webkit-scrollbar-thumb:hover {
	background-color: #b6becc;
	height: 50px;
	-webkit-border-radius: 4px;
}
/*---滚动条大小--*/
::-webkit-scrollbar {
	width: 12px;
	height: 12px;
}
/*---滚动框背景样式--*/
::-webkit-scrollbar-track-piece {
	background-color: #e4e6e9;
	-webkit-border-radius: 15PX;
}
::-webkit-scrollbar-thumb:active {
	height: 50px;
	background-color: #b6becc;
	-webkit-border-radius: 6px;
}

/**============    按钮样式---begin          ============**/
.btnGroup {
	clear: both;
	border-top: 1px solid #f1f1f1;
	text-align: right;
	padding-top: 10px;
	padding-bottom: 10px;
}
/*取消按钮，按钮的基本样式*/
.btn {
	display: inline-block;
	height:30px;
	line-height:30px;
	text-align: center;
	padding:0 20px;
	margin:0 5px;
	background-color: #fff;
	border: 1px solid #9ba4b3;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	font-size: 14px;
	color: #3d4451;
	cursor: pointer;
}

.pointer {
	cursor: pointer;
}


.btn:hover {
	background-color: #3ccdb3;
	border: 1px solid #3ccdb3;
	color:#fff;
}

/*禁用按钮样式*/
.btn.disabled,
.btn[disabled] {
	background-color: #c9cdd6;
	border: 1px solid #c9cdd6;
	color:#fff;
	cursor: default;
}


/*确认，保存的按钮样式*/
.btn-primary {
	background-color: #4585f5;
	border:1px solid #4585f5;
	color:#fff;
}
.btn-primary:hover {
	background-color: #3b78e4;
	border:1px solid #3b78e4;
	color:#fff;
}

/*删除按钮样式*/
.btn-primary-delete {
	background-color: #f85c5c;
	border:1px solid #f85c5c;
	color:#fff;
}
.btn-primary-delete:hover {
	background-color: #eb4c4c;
	border:1px solid #eb4c4c;
	color:#fff;
}

/**============    在线聊天---begin    ============**/
.online{
	height:100%;
	position: relative;
}

/*左边（会话、客服）*/
.online .online-left{
	width:350px;
	height:100%;
	float:left;
	border-right:1px solid #f1f1f1;
	position: relative;
}
.online .online-left .tit{
	height:50px;
	line-height: 50px;
	background-color:#fff;
	border-bottom: 1px solid #f1f1f1;
}
.online .online-left .tit ul li{
	float:left;
	width:80px;
	height:50px;
	line-height: 50px;
	border-bottom:1px solid #f1f1f1;
	border-left:1px solid #f1f1f1;
	font-size:14px;
	color:#929aa8;
	text-align: center;
	cursor: pointer;
}
.online .online-left .tit ul li:hover,
.online .online-left .tit ul li.active{
	background-color:#f9f9f9;
	color:#323842;
	border-bottom:1px solid #f9f9f9;
}


/*会话列表*/
.online .online-left .peopleList{
	background: #f9f9f9;
	position: absolute;
	top:50px;
	bottom:0;
	/* bottom:130px; */
	left: 0;
	right: 0;
	overflow-y: auto;
}
.online .online-left .peopleList ul li{
	width:100%;
	height:70px;
	border-bottom:1px solid #f1f1f1;
	position:relative;
}
.online .online-left .peopleList ul li .people{
	padding:13px 16px;
}
.online .online-left .peopleList ul li .people .name{
	height:22px;
	line-height: 22px;
	font-size:14px;
	font-weight:bold;
	color:#323842;
}
.online .online-left .peopleList ul li .people .name .time{
	float:right;
	color:#9ba4b3;
	font-size:14px;
	font-weight: normal;
}
.online .online-left .peopleList ul li .people .text{
	height:22px;
	line-height: 22px;
	font-size:14px;
	color:#929aa8;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	position: relative;
	padding-right: 25px;
}
.online .online-left .peopleList ul li .people .number{
	width:16px;
	height:16px;
	line-height: 16px;
	text-align: center;
	background: #f85c5c;
	border-radius: 50%;
	color:#fff;
	font-style: normal;
	position: absolute;
	right:0;
	top:5px;
	display: none;
}


/*关闭某个会话列表样式*/
.online .online-left .peopleList ul li .closeLi{
	width:18px;
	height:18px;
	background:url(../img/closeLi.png) center center no-repeat;
	position:absolute;
	right:15px;
	bottom:12px;
	cursor:pointer;
	z-index:9;
	display:none;
}
.online .online-left .peopleList ul li .closeLi:hover{
	background:url(../img/closeLiHover.png) center center no-repeat;
}


/*会话列表滑过样式*/
.online .online-left .peopleList ul li:hover{
	background: #f2f3f5;
}
.online .online-left .peopleList ul li:hover .closeLi{
	display:block;
}
/*新消息*/
.online .online-left .peopleList ul li.news .people .number{
	display:block;
}
/*会话列表选中样式*/
.select-background {
	background-color: #f2f3f5;
}


