*{
	font-family: "微软雅黑","Microsoft YaHei";
	-moz-box-sizing: border-box; /*Firefox3.5+*/
	-webkit-box-sizing: border-box; /*Safari3.2+*/
	-o-box-sizing: border-box; /*Opera9.6*/
	-ms-box-sizing: border-box; /*IE8*/
	box-sizing: border-box;
	outline: none;
}
html {
	width: 100%;
	height:100%;
	padding: 0;
	margin: 0;
}

body {
	padding: 0;
	margin: 0;
	height:100%;
	width: 100%;
	background-color: #fff;
}

img {
	border: none;
	display: inline;
}

dl,dd,dt,ul,li {
	padding: 0;
	margin: 0;
	list-style: none;
}

h1,h2,h3,h4,h5 {
	margin: 0;
	padding: 0;
}

a:hover,a:active {
	outline: 0;
}
a {
	color: #0088cc;
	text-decoration: none;
}
a:hover {
	color: #005580;
	text-decoration: none;
}
/*清除a链接自带虚线*/
a {
	blr:expression(this.onFocus=this.blur());
}
a {
  outline: none;
}

label {
	display: block;
	font-size: 14px;
}
h1,h2,h3,h4,h5,h6 {
	font-size: 12px;
	font-weight: normal;
	padding: 0;
	margin: 0;
}
select,input,textarea,p.text {
	border: 1px solid #ccc;
	background-color:#fff;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	padding: 0;
	color: #333;
	outline:none;
}
textarea {
	min-height:90px;
	line-height: 20px;
	resize: none;
	resize:vertical;
	font-size: 12px;
}
