/*中间（聊天记录）*/
.online .online-center{
	height:100%;
	padding:0 15px;
	margin-left: 351px;
	margin-right: 440px;
	position: relative;
}
.online .online-center .tit{
	height:50px;
	line-height: 50px;
	background-color:#fff;
	border-bottom: 1px solid #f1f1f1;
}
.online .online-center .tit span{
	float:left;
	font-size: 14px;
	color:#323842;
}
.online .online-center .tit div{
	float:right;
	margin-right:18px;
}
.online .online-center .tit div img{
	margin-top: 9px;
	cursor: pointer;
}


/*消息显示区域*/
.online .online-center .chatDiv{
	position: absolute;
	bottom: 220px;
	top: 50px;
	left: 15px;
	right: 15px;
	overflow-y: auto;
}
.online .online-center .chatDiv .messageBox{
	height:100%;
	box-sizing:border-box;
	padding-top:20px;
}
.online .online-center .chatDiv .messageBox{
	width:100%;
	padding: 10px 10px;
	display: inline-block;
}
.online .online-center .chatDiv .messageBox p{
	clear:both;
	text-align: center;
	font-size: 12px;
	color:#c0c0c0;
	margin:15px 0;
}
.online .online-center .chatDiv .message {
	clear: both;
	padding-bottom:15px;
}
.online .online-center .chatDiv .message .text {
	position: relative;
	margin:0 68px;
	margin-top:5px;
	border-radius:4px;
	padding:8px 6px;
	line-height: 24px;
	float: left;
	font-size: 14px;
	color:#333;
}
.online .online-center .chatDiv .message .text p{
	margin:0;
	font-size: 14px;
	color:#333;
}

/*客服*/
.online .online-center .chatDiv .message.service{
	float:left;
	background: url("../img/service.png") left top no-repeat;
}
.online .online-center .chatDiv .message.service .text {
	background: #f2f3f5;
	color:#323842;
	word-break: break-all;
}
.online .online-center .chatDiv .message.service .text p{
	text-align:left;
}
.online .online-center .chatDiv .message.service .text .service-left{
	display: inline-block;
	width:8px;
	height:14px;
	background: url("../img/service-left.png");
	position: absolute;
	left: -8px;
	top: 14px;
}

/*客户*/
.online .online-center .chatDiv .message.customer{
	float:right;
	background: url("../img/customer.png") right top no-repeat;
}
.online .online-center .chatDiv .message.customer .text {
	background: #f0f5ff;
	word-break: break-all;
}
.online .online-center .chatDiv .message.customer .text p{
	text-align:left;
}
.online .online-center .chatDiv .message.customer .text .customer-right{
	display: inline-block;
	width:8px;
	height:14px;
	background: url("../img/customer-right.png");
	position: absolute;
	right: -8px;
	top: 14px;
}

/**	客户端	*/
/*客服*/
.online .online-center .chatDiv .agent{
	float:left;
	background: url("../img/customer.png") left top no-repeat;
}
.online .online-center .chatDiv .agent .text {
	background: #f2f3f5;
	color:#323842;
	word-break: break-all;
}
.online .online-center .chatDiv .agent .text p{
	text-align:left;
}
.online .online-center .chatDiv .agent .text .agent-left{
	display: inline-block;
	width:8px;
	height:14px;
	background: url("../img/service-left.png");
	position: absolute;
	left: -8px;
	top: 14px;
}
/*客户*/
.online .online-center .chatDiv .client{
	float:right;
	background: url("../img/service.png") right top no-repeat;
}
.online .online-center .chatDiv .client .text {
	background: #f0f5ff;
	word-break: break-all;
}
.online .online-center .chatDiv .client .text p{
	text-align:left
}
.online .online-center .chatDiv .client .text .client-right{
	display: inline-block;
	width:8px;
	height:14px;
	background: url("../img/customer-right.png");
	position: absolute;
	right: -8px;
	top: 14px;
}


/*消息发送区域*/
.online .online-center .sendDiv{
	height:220px;
	border-top: 1px solid #dddddd;
	position: absolute;
	bottom: 0;
	left: 15px;
	right: 15px;
}
/*输入框*/
.online .online-center .sendDiv textarea{
	width:100%;
	height:150px;
	color:#666;
	border: none;
	margin-top:10px;
}
/*操作列表   发送文字、表情、文件、图片*/
.online .online-center .sendDiv .operation{
	height:30px;
}
.online .online-center .sendDiv .operation a{
	display:inline-block;
	width:20px;
	height:20px;
	margin:5px 3px;
	cursor:pointer;
}
.online .online-center .sendDiv .operation a.operationFace{
	background:url(../img/face.png) center center no-repeat;
}
.online .online-center .sendDiv .operation a.operationFace:hover{
	background:url(../img/faceHover.png) center center no-repeat;
}
.online .online-center .sendDiv .operation a.operationFile{
	background:url(../img/file.png) center center no-repeat;
}
.online .online-center .sendDiv .operation a.operationFile:hover{
	background:url(../img/fileHover.png) center center no-repeat;
}
.online .online-center .sendDiv .operation a.operationImage{
	background:url(../img/image.png) center center no-repeat;
}
.online .online-center .sendDiv .operation a.operationImage:hover{
	background:url(../img/imageHover.png) center center no-repeat;
}



/*发送、清屏按钮*/
.online .online-center .sendDiv .btnDiv{
	float:right;
	position: relative;
}
.online .online-center .sendDiv .btnDiv .btn{
	float: right;
	margin-right:15px;
	height:30px;
	line-height: 28px;
	padding:0 26px;
	padding-left:31px;
	background: #fff;
	border:1px solid #9ba4b3;
	color:#3d4451;
	font-size: 14px;
	border-radius: 4px;
	cursor: pointer;
}
.online .online-center .sendDiv .btnDiv .btn-send{
	background:#4585f5;
	border:1px solid #4585f5;
	color:#fff;
}
.online .online-center .sendDiv .btnDiv .tips{
	width:222px;
	height:50px;
	line-height:45px;
	background:url("../img/tipsBg.png") no-repeat;
	position:absolute;
	right:-90px;
	bottom:35px;
	font-size:12px;
	color:#222;
	text-indent:8px;
	display: none;
}
.online .online-center .sendDiv .btnDiv .tips img{
	float: left;
	margin-top: 12px;
	margin-left: 10px;
}
.show{
	display: block;
}


/*右边（常用语、历史记录）*/
.online .online-right{
	width:440px;
	height:100%;
	float:right;
	border-left:1px solid #f1f1f1;
	position: relative;
}
.online .online-right .tit{
	height:50px;
	line-height: 50px;
	background-color:#fff;
	border-bottom: 1px solid #f1f1f1;
	padding:0 16px;
}
.online .online-right .tit ul li{
	display: inline-block;
	float:left;
	line-height: 47px;
	font-size: 14px;
	color:#929aa8;
	padding:0 15px;
	margin-right:15px;
	cursor: pointer;
	border-bottom: 3px solid transparent;
}
.online .online-right .tit ul li:hover,
.online .online-right .tit ul li.active{
	color:#323842;
	border-bottom: 3px solid #4585f5;
}
.online .online-right .online-right-content{
	position: absolute;
	bottom: 0;
	top: 50px;
	left: 10px;
	right: 10px;

}


/*常用语列表*/
.online .online-right .online-right-content .langDiv{
	overflow-y: auto;
	position: absolute;
	top: 0px;
	bottom: 0;
	left: 0;
	right: 0;
}
.online .online-right .online-right-content .langDiv ul li{
	width:100%;
	font-size: 14px;
	color:#929aa8;
	line-height: 20px;
	padding:10px 5px;
	border-bottom: 1px solid #f1f1f1;
	cursor:pointer;
}
.online .online-right .online-right-content .langDiv ul li:hover{
	background-color:#f2f3f5;
}


/*聊天记录列表*/
.historyRecord{
	padding:20px 5px;
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:50px;
	overflow-y:auto;
}
.historyRecord .historyList{
	margin-bottom:15px;
}
.historyRecord .historyList .timeName strong{
	color:#929aa8;
	font-size:12px;
	font-weight:bold;
	margin-right:10px;
} 
.historyRecord .historyList p{
	color:#929aa8;
	font-size:12px;
	line-height:20px;
}

.historyRecord .historyList.client .timeName strong{
	color:#333;
}


/*聊天记录翻页*/
.pagination{
	position:absolute;
	bottom:22px;
}
.pagination a{
	display:inline-block;
	float:left;
	width:28px;
	height:28px;
	cursor:pointer;
}
.pagination a.prev{
	background:url(../img/prev.png) center center no-repeat;
}
.pagination a.next{
	background:url(../img/next.png) center center no-repeat;
}
.pagination span{
	color:#929aa8;
	font-size:14px;
	margin-left:15px;
	line-height:28px;
}
.pagination span b{
	font-weight:normal;
}


/*当前等待提示动图样式*/
.coverWait{
	position:absolute;
	left:0;
	right:0;
	top:0;
	bottom:0;
	z-index:10;
	background:rgba(0,0,0,.3);
	display:none;
}
.coverWait .wait{
	width:260px;
	background:#fff;
	border-radius:6px;
	padding:30px 0;
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-130px;
	margin-top:-100px;
	text-align:center;
}
.coverWait .wait p{
	color:#929aa8;
	font-size:14px;
	margin-top:-50px;
}
.coverWait .wait p span{
	color:#333;
}
/*1400分辨率以上*/
@media only screen and (min-width: 1400px) {
	.online .online-left{
		width:350px;
	}
	.online .online-right{
		width:440px;
	}
	.online .online-center{
		margin-left: 350px;
		margin-right: 440px;
	}
}
/*1280分辨率以上（大于1200px 小于1400）*/
@media screen and (min-width: 1200px) and (max-width: 1400px) {
	.online .online-left{
		width:280px;
	}
	.online .online-right{
		width:360px;
	}
	.online .online-center{
		margin-left: 280px;
		margin-right: 360px;
	}
}
/*1180分辨率以上（小于1200px 大于800）*/
@media screen and (min-width: 1000px) and (max-width: 1200px) {
	.online .online-left{
		width:260px;
	}
	.online .online-right{
		width:260px;
	}
	.online .online-center{
		margin-left: 260px;
		margin-right: 260px;
	}
	.online .online-right .tit ul li{
		padding:0 10px;
		margin-right:10px;
	}
}
/*1280分辨率以上（小于1200px 大于800）*/
@media screen and (min-width: 800px) and (max-width: 1000px) {
	.online .online-left{
		width:180px;
	}
	.online .online-right{
		width:240px;
	}
	.online .online-center{
		margin-left: 180px;
		margin-right: 240px;
	}
	.online .online-right .tit ul li{
		padding:0 6px;
		margin-right:4px;
	}
}
@media screen and (max-width: 800px){
	.online{
		width:801px;
	}
	.online .online-left{
		width:180px;
	}
	.online .online-right{
		width:240px;
	}
	.online .online-center{
		margin-left: 180px;
		margin-right: 240px;
	}
	.online .online-right .tit ul li{
		padding:0 6px;
		margin-right:4px;
	}
}

/* 表情样式 */
.face{
	width:800px;
	padding:10px;
	box-sizing:border-box;
	display:block;
	border: '1';
	border-color: black;
	position: absolute;
	top: -135px;
	background:#fff;
	box-shadow:0 0 20px rgba(0,0,0,0.5);
	border-radius:5px;
}


/*图片放大的样式*/
.zoomInContainer{
	width:100%;
	height:100%;
	position: fixed;
	top:0;
	left:0;
	right:0;
	bottom:0;
	z-index: 9999;
	padding:20px;
}
.zoomInContainer .zoomInContent{
	position: fixed;
	top:20px;
	left:20px;
	right:20px;
	bottom:20px;
	background:url(../img/cover03.png);
	z-index: 9999;
	
}
.zoomInContainer .zoomInContent .historyImg{
	width:100%;
	height:100%;
	overflow: hidden;
	position: relative;
}
.zoomInContainer .zoomInContent .historyImg img{
	max-height:100%;
	max-width: 90%;
	position: absolute;
	top:50%;
	left:50%;
	transform:translate(-50%,-50%);
}
/*禁止点击*/
.zoomInContainer .zoomInContent .disable{
	opacity: 0.1;
    filter: alpha(opacity=10);
}
.zoomInContainer .zoomInContent .disable:hover{
	opacity: 0.1;
    filter: alpha(opacity=10);
}
/*关闭按钮*/
.zoomInContainer .zoomInContent .zoomInClose{
	width:40px;
	height:40px;
	position: absolute;
	top:5px;
	right:5px;
	line-height: 40px;
	font-size: 30px;
	text-align:center;
	z-index: 10010;
	cursor: pointer;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	background-image: url(../img/zoomInClose.png);
	background-repeat: no-repeat;
	background-position: center center;
}
.zoomInContainer .zoomInContent .zoomInClose:hover{
	background-image: url(../img/zoomInCloseActive.png);
}



