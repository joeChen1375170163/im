.ztree * {
    padding: 0;
    margin: 0;
    font-size: 14px;
    font-family: Verdana, Arial, Helvetica, AppleGothic, sans-serif
}

.ztree {
    margin: 0;
    padding: 5px;
    color: #333
}

.ztree li {
    padding: 0;
    margin: 0;
    list-style: none;
    line-height: 14px;
    text-align: left;
    white-space: nowrap;
    outline: 0
}

.ztree li ul {
    margin: 0;
    /* padding: 0 0 0 18px; */
}
.ztree li ul li a{
	padding:10px;
	/* padding-left:18px; */
}
.ztree li ul.line {
    /* background: url(../images/ztree/line_conn.gif) 0 0 repeat-y; */
}

.ztree li a {
    padding: 10px 10px;    
    margin: 0;
    cursor: pointer;
    color: #333;
    background-color: transparent;
    text-decoration: none;
    /* vertical-align: top; */
    display: inline-block;
    line-height: 16px;
    width:100%;
}

.ztree li a:hover {
	background-color: #dfe6f3;
	color:#2758ae;
}
/*点击编辑按钮后页面弹出的可编辑的input窗样式*/
.ztree li a.curSelectedNode_Edit {
    background-color: #FFE6B0;
}

.ztree li a.tmpTargetNode_inner {
    padding-top: 0px;
    background-color: #316AC5;
    color: white;
    height: 16px;
    border: 1px #316AC5 solid;
    opacity: 0.8;
    filter: alpha(opacity=80)
}

.ztree li a input.rename {
    height: 22px;
    width: 100px;
    padding: 0;
    margin: 0;
    font-size: 14px;
    line-height:22px;
    border: 1px #7EC4CC solid;
    *border: 0px
}

.ztree li span {
    line-height: 16px;
    margin-right: 2px
}
.ztree li span.node_name{
	width:150px; 
	vertical-align:middle;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	display:inline-block;
}
.ztree li span.button {
    line-height: 0;
    margin: 0;
    width: 16px;
    height: 16px;
    display: inline-block;
    vertical-align: middle;
    border: 0 none;
    cursor: pointer;
    outline: none;    
}
/*选择框的宽高*/
.ztree li span.button.chk {
    width: 16px;
    height: 16px;
    margin: 0 3px 0 0;
    cursor: auto;
    margin-right: 4px;
}
/*选择框的字体图标的整体样式   默认样式*/
.ztree li span.button.chk.checkbox_false_full,
.ztree li span.button.chk.checkbox_false_part,
.ztree li span.button.chk.radio_false_full,
.ztree li span.button.chk.radio_false_part{
	font-size: 16px;
	color:#bac0d0;
}

.ztree li span.button.chk.checkbox_false_full_focus,
.ztree li span.button.chk.checkbox_false_part_focus,
.ztree li span.button.chk.radio_false_full_focus,
.ztree li span.button.chk.radio_false_part_focus{
	font-size: 16px;
	color:#8b9fd2;
}
.ztree li span.button.chk.radio_true_part,
.ztree li span.button.chk.checkbox_true_part{
	font-size: 16px;
	color:#4982e8;
}
.ztree li span.button.chk.checkbox_true_part_focus,
.ztree li span.button.chk.radio_true_part_focus{
	font-size: 16px;
	color:#2758ae;
}
.ztree li span.button.chk.checkbox_true_full,
.ztree li span.button.chk.radio_true_full{
	color:#2758ae;
	font-size: 16px;
}
.ztree li span.button.chk.checkbox_true_full_focus,
.ztree li span.button.chk.radio_true_full_focus{
	color:#0f3f94;
	font-size: 16px;
}


.ztree li span.button.chk.checkbox_false_disable,
.ztree li span.button.chk.checkbox_true_disable,
.ztree li span.button.chk.radio_false_disable,
.ztree li span.button.chk.radio_true_disable{
	font-size: 16px;
	color:#cccccc;
	cursor: default;
}


.ztree li span.button.chk.checkbox_false_full:before,
.ztree li span.button.chk.checkbox_false_full_focus:before,
.ztree li span.button.chk.checkbox_false_part:before,
.ztree li span.button.chk.checkbox_false_part_focus:before{
    content:"\e939";
}


.ztree li span.button.chk.checkbox_true_full:before,
.ztree li span.button.chk.checkbox_true_full_focus:before {
    content:"\e938";
}

.ztree li span.button.chk.checkbox_true_part:before,
.ztree li span.button.chk.checkbox_true_part_focus:before{
    content:"\e937";
}

.ztree li span.button.chk.checkbox_false_disable:before{
    content:"\e939";
}
.ztree li span.button.chk.checkbox_true_disable:before{
	content:"\e938";
}


.ztree li span.button.chk.radio_false_full:before,
.ztree li span.button.chk.radio_false_full_focus:before,
.ztree li span.button.chk.radio_false_part:before,
.ztree li span.button.chk.radio_false_part_focus:before {
    content:"\e973";
}

.ztree li span.button.chk.radio_true_full:before,
.ztree li span.button.chk.radio_true_full_focus:before {
    content:"\e972";
}

.ztree li span.button.chk.radio_true_part:before,
.ztree li span.button.chk.radio_true_part_focus:before {
    content:"\e971"; 
}
.ztree li span.button.chk.radio_false_disable:before {
    content:"\e973";
}
.ztree li span.button.chk.radio_true_disable:before {
    content:"\e972";
}



.ztree li span.button.switch {
    width: 18px;
    height: 18px;
    display: none;
}

.ztree li span.button.ico_close,
.ztree li span.button.ico_open {
    margin-right: 2px;
    font-size: 16px;
    color:#2758ae;
    line-height: 16px;
}
.ztree li span.button.ico_close:before,
.ztree li span.button.ico_open:before{
	content: "\e99f";
}
.ztree li a.level1 span.button.ico_close,
.ztree li a.level1 span.button.ico_open{
	margin-right: 2px;
    font-size: 12px;
    color:#2758ae;
    line-height: 16px;
}
.ztree li a.level1 span.button.ico_close:before{
	content: "\e9a0";
}
.ztree li a.level1 span.button.ico_open:before{
	content: "\e99d";
}


.ztree li span.button.ico_docu {
    margin-right: 2px;
    vertical-align: middle;
    margin-left: 10px;
    color: #3e70c9;
    line-height: 18px;
    font-size: 4px;
    display: none;
}
.ztree li span.button.ico_docu:before {
    content:"\e944";
}

/*增删改的按钮*/
.ztree li span.button.add,
.ztree li span.button.edit,
.ztree li span.button.remove{
	font-size: 17px;
	margin: 0 4px;
	color:#9abeff;
	cursor: pointer;
}
.ztree li span.button.add{
	margin-left: 15px;
}
.ztree li span.button.add:hover,
.ztree li span.button.edit:hover,
.ztree li span.button.remove:hover{
	color:#fff;
}
.ztree li span.button.add:before{
	content:"\e99b"; 
}
.ztree li span.button.edit:before{
	content:"\e99e"; 
}
.ztree li span.button.remove:before{
	content:"\e99c"; 
}

.ztree li span.button.ico_loading {
    margin-right: 2px;
    background: url(../images/ztree/loading.gif) no-repeat scroll 0 0 transparent;
    vertical-align: top;
    *vertical-align: middle
}

ul.tmpTargetzTree {
    background-color: #FFE6B0;
    opacity: 0.8;
    filter: alpha(opacity=80)
}

span.tmpzTreeMove_arrow {
    width: 16px;
    height: 16px;
    display: inline-block;
    padding: 0;
    margin: 2px 0 0 1px;
    border: 0 none;
    position: absolute;
    background-color: transparent;
    background-repeat: no-repeat;
    background-attachment: scroll;
    background-position: -110px -80px;
}

ul.ztree.zTreeDragUL {
    margin: 0;
    padding: 0;
    position: absolute;
    width: auto;
    height: auto;
    overflow: hidden;
    background-color: #cfcfcf;
    border: 1px #00B83F dotted;
    opacity: 0.8;
    filter: alpha(opacity=80)
}

.zTreeMask {
    z-index: 10000;
    background-color: #cfcfcf;
    opacity: 0.0;
    filter: alpha(opacity=0);
    position: absolute
}

/*ͼ��Ϊ����ͼ��*/
.ztree li span.button{
    font-family: 'icomoon' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.ztree li span.button.root_open:before  {
    background-position: -92px -54px
}

.ztree li span.button.root_close:before  {
    background-position: -74px -54px
}

.ztree li span.button.roots_open:before  {
    background-position: -92px 0
}

.ztree li span.button.roots_close:before  {
    background-position: -74px 0
}

.ztree li span.button.center_open:before  {
    background-position: -92px -18px
}

.ztree li span.button.center_close:before {
    content: "\e93a";
    font-size: 18px;
    color: #91b8fc;

}

.ztree li span.button.bottom_open:before  {
    background-position: -92px -36px
}

.ztree li span.button.bottom_close:before  {
    background-position: -74px -36px
}

.ztree li span.button.noline_open.level0:before {
    content: "\e99e";
    font-size: 16px;
    color: #3e70c9;
    line-height: 18px;
}

.ztree li span.button.noline_close.level0:before {
    content: "\e99e";
    font-size: 16px;
    color: #3e70c9;
    line-height: 18px;
}
.ztree li span.button.noline_open.level1:before {
    content: "\e979";
    font-size: 10px;
    color: #3e70c9;
    line-height: 18px;
}

.ztree li span.button.noline_close.level1:before {
    content: "\e985";
    font-size: 12px;
    color: #3e70c9;
    line-height: 18px;
}

.ztree li span.button.root_docu:before  {
    background: none;
}

.ztree li span.button.roots_docu:before  {
    background-position: -56px 0
}

.ztree li span.button.center_docu:before  {
    background-position: -56px -18px
}

.ztree li span.button.bottom_docu:before  {
    background-position: -56px -36px
}

.ztree li span.button.noline_docu:before {
    background: none;
}
.icon-sign:before {
    content: "\e96c";
    font-size: 12px;
    color: #3e70c9;
    line-height: 18px;
    float: right;
}
/*展开节点的父节点位选中状态*/
.ztree li span.button.level0.switch.noline_open+a.level0{
	background-color:#dfe6f3; 
}
.ztree li span.button.level0.switch.noline_open+a.level0 span{
	color:#2758ae;
}

.ztree li a.curSelectedNode{
    color: #ffffff;
    background-color: #2758ae;
    width:100%;
    padding-right:30px;
	background-image:url('data:image/png;base64,');
	background-position: right center;
	background-repeat: no-repeat;
}
.ztree li a.curSelectedNode span.button.ico_close,
.ztree li a.curSelectedNode span.button.ico_open,
.ztree li a.curSelectedNode span.button.chk,
.ztree li a.curSelectedNode.level1 span.button.ico_close,
.ztree li a.curSelectedNode.level1 span.button.ico_open{
    color: #FFFFFF;
}
.ztree li a:hover span.button.chk{
    color: #2758ae;
}
.ztree li a.curSelectedNode:hover span.button.chk{
    color: #FFFFFF;
}

.ztree li ul{
    padding-bottom: 6px;
    padding-top: 6px;
    padding-left:20px;
    box-sizing:border-box;
}
.ztree li.level0{
    border-bottom: 1px solid #e7ecf1;
}

#addSiteList  #addSiteList_1_a{
	width:322px;
}